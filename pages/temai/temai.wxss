/* pages/temai/temai.wxss */
page{
	background: #f0f0f0;
	border-top: 2rpx solid #f0f0f0;
}
.goods{
	background: #fff;
	margin-bottom: 10rpx;
}
.goods_img{
	width: 200rpx;
	height: 200rpx;
	margin: 26rpx 20rpx;
}
.goods_msg{
	margin-right: 20rpx;
}
.goods_name{
	font-size: 32rpx;
	color: #333333;
	line-height: 50rpx;
	margin-top: 34rpx;
	height: 100rpx;
	display: block;
	overflow: hidden;
}
.goods_l{
	float: left;
}
.goods_pri{
	font-size: 36rpx;
	color: #f23030;
}
.goods_pri text{
	font-size: 24rpx;
	padding: 6rpx 10rpx;
	border: 2rpx solid #f23030;
	border-radius: 10rpx;
	margin-left: 20rpx;
}
.goods_opri{
	display: block;
	font-size: 24rpx;
	color: #999;
	margin-top: 12rpx;
}
.goods_btn{
	float: right;
	width: 140rpx;
	height: 60rpx;
	text-align: center;
	color: #fff;
	background: #f23030;
	border-radius: 10rpx;
	font-size: 24rpx;
	line-height: 60rpx;
	margin-top: 14rpx;
}
.shop_msg{
	width: 100%;
	height: auto;
	padding-top: 60rpx;
	background: #fff;
	margin-bottom: 20rpx;
}
.shop_img{
	width: 100rpx;
	height: 100rpx;
	border-radius: 50rpx;
	display: block;
	margin: 0 auto;
	border: 4rpx solid pink;
}
.shop_name{
	font-size: 28rpx;
	color: #333;
	margin-top: 20rpx;
	display: block;
	text-align: center;
	border-bottom: 2rpx solid #f5f5f5;
	padding-bottom: 20rpx;
}
.shop_ranking{
	display: flex;
	height: 80rpx;
	line-height: 80rpx;
}
.shop_ranking view{
	display: block;
	text-align: center;
	flex: 1;
	color: #333;
	font-size: 28rpx;
}
.ranking_r{
	color: #F40;
}
.ranking_g{
	color: #00BA97;
}