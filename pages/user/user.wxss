page{
  background: #f0f0f0;
}
.mine_top{
  padding: 0 20rpx;
  height: 224rpx;
  background: #fff;
  padding-top: 16rpx;
  border-top: 2rpx solid #f0f0f0;
}
.mt_wrap{
  width: 100%;
  height: 208rpx;
  display: flex;
  background: #f6584c;
  border-radius: 20rpx;
}
.mtw_l{
  width: 140rpx;
  height: 126rpx;
  margin: 44rpx 0 0 24rpx;
  position: relative;
}
.mtw_prot{
  width: 108rpx;
  height: 108rpx;
  border-radius: 54rpx;
  border:2rpx solid #f48c8d;
  margin-left: 10rpx;
}
.mtw_lev{
  position: absolute;
  bottom: 0;
  left: -10rpx;
  width: 140rpx;
  height: 28rpx;
}
.lev_txt{
  width: 140rpx;
  height: 28rpx;
  border-radius: 17rpx;
  font-size: 24rpx;
  line-height: 28rpx;
  text-indent: 32rpx;
  background: #9f3738;
  display: block;
  color: #fff;
}
.lev_img{
  position: absolute;
  left: 0;
  bottom: 0;
  width: 28rpx;
  height: 28rpx;
}
.mtw_r{
  flex: 1;
}
.mwr_name{
  font-size: 28rpx;
  margin-top: 56rpx;
  color: #fff;
}
.mwr_id{
  font-size: 24rpx;
  color: #facac8;
  margin-top: 28rpx;
}
.mine_btm{
  margin-top: 20rpx;
  padding-left: 20rpx;
  background: #fff;
}
.mb_list{
  width: 100%;
  height: 76rpx;
  border-bottom: 1px solid #f0f0f0;
  display: flex;
}
.mbl_icon{
  width: 30rpx;
  height: 30rpx;
  margin: 24rpx 20rpx 0 0;
}
.mbl_txt{
  flex: 1;
  font-size: 28rpx;
  color: #343434;
  line-height: 76rpx;
}
.mbl_arrow{
  width: 16rpx;
  height: 25rpx;
  padding: 28rpx 20rpx 0 0;
}
.mbg_count{
  float: right;
  font-size: 24rpx;
  color: #f23030;
  margin-right: 10rpx;
}